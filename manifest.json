{
  "display_information": {
    "name": "Omnibot",
    "description": "Soule-Reeves potpourri of bottery",
    "background_color": "#b07920"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": true,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "omnibot",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/bid",
        "description": "Sealed bidding and wisdom-of-crowds-ing",
        "usage_hint": "Your bid or other commentary to be revealed later",
        "should_escape": true
      },
      {
        "command": "/roll",
        "description": "Dice rolling",
        "usage_hint": "An integer",
        "should_escape": true
      },
      {
        "command": "/omninom",
        "description": "Reference implementation of an Omnibot slash command",
        "usage_hint": "[This is the usage hint: type something]",
        "should_escape": true
      },
      {
        "command": "/omnecho",
        "description": "Just for debugging",
        "usage_hint": "[This is the usage hint: type something]",
        "should_escape": true
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "user": [
        "im:write"
      ],
      "bot": [
        "channels:history",
        "channels:read",
        "chat:write",
        "commands",
        "groups:history",
        "im:history",
        "im:read",
        "im:write",
        "mpim:history",
        "mpim:read",
        "users:read",
        "groups:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "user_events": [
        "app_home_opened"
      ],
      "bot_events": [
        "message.channels",
        "message.mpim"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
