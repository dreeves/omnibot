{
  "name": "omnibot",
  "version": "2024.07.19-a",
  "description": "A Discord & Slack bot that plays games and mediates auctions",
  "main": "app.js",
  "scripts": {
    "build": "cd ./public && npx snowpack build && npm run register",
    "start": "npm run build && node app.js",
    "register": "node register-commands.js",
    "test": "mocha --require mocha-suppress-logs quals"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@slack/bolt": "^3.15.0",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "chai-as-promised": "^7.1.1",
    "discord-api-types": "^0.37.65",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-emoji": "^2.1.3",
    "random-word-slugs": "^0.1.7",
    "remarkable": "^2.0.1",
    "ws": "^8.14.2"
  },
  "engines": {
    "node": "18"
  },
  "repository": {
    "url": "https://github.com/dreeves/omnibot"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "chat bots"
  ],
  "devDependencies": {
    "chai": "^4.3.10",
    "chromedriver": "^119.0.1",
    "mocha": "^10.2.0",
    "mocha-suppress-logs": "^0.4.1",
    "proxyquire": "^2.1.3",
    "puppeteer": "^21.5.2",
