    "node",
    "chat bots"
  ],
  "devDependencies": {
    "chai": "^5.1.1",
    "chromedriver": "^126.0.5",
    "mocha": "^10.6.0",
    "mocha-suppress-logs": "^0.5.1",
    "proxyquire": "^2.1.3",
    "puppeteer": "^22.13.1",
