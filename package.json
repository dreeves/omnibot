{
  "name": "omnibot",
  "version": "2023.09.22-a",
  "description": "A Discord & Slack bot that plays games and mediates auctions",
  "main": "app.js",
  "scripts": {
    "build": "cd ./public && npx snowpack build && npm run register",
    "start": "npm run build && node app.js",
    "register": "node register-commands.js",
    "test": "mocha --require mocha-suppress-logs quals"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "@slack/bolt": "^3.13.1",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "chai-as-promised": "^7.1.1",
    "discord-api-types": "^0.37.42",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-emoji": "^1.11.0",
    "random-word-slugs": "^0.1.7",
    "remarkable": "^2.0.1",
    "ws": "^8.13.0"
  },
  "engines": {
    "node": "18"
  },
  "repository": {
    "url": "https://github.com/dreeves/omnibot"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "chat bots"
  ],
  "devDependencies": {
    "chai": "^4.3.7",
    "chromedriver": "^113.0.0",
    "mocha": "^10.2.0",
    "mocha-suppress-logs": "^0.3.1",
    "proxyquire": "^2.1.3",
    "puppeteer": "^20.2.0",
    "selenium-webdriver": "^4.9.2",
    "sinon": "^15.2.0",
    "snowpack": "^3.2.2"
  }
